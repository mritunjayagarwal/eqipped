              url: "autocomplete/",
              dataType: "jsonp",
              type: "GET",
              data: req,
              success: function (data) {
                res(data)
              },
              error: function (err) {
                console.log(err.status)
              }
            });
          },
          minLength: 1,
          select: function (event, ui) {
            if (ui.items) {
              $('#searchProduct').text(ui.item.label);
            }
          }
        });
      });
